
def TopolGen(molecule,zval,inout,endline):
    
    """
    assumes  the following fileto and filefrom 
    
    
    filefrom = '/media/kselvon/D20492AA049290D9/Iridis-runs/directedruns/iridis-folder-copies/'+molecule+'/drag'+inout+'z'+str(zval)+'_4fs'
    
    fileto = '/media/kselvon/7A82B43582B3F42B/iridisruns2019/trajectoriesForMD/
    
    
    and the following nameing scheme 
    
    amilorideDMPC_dragin.pdb (multi) is made into amiloride_DG_topol_z0.pdb
    amiloride_dragin_xtc.xtc is copied over to new folder 
    
    
    """
    
    #set file paths to and from 
    #filefrom = '/media/kselvon/D20492AA049290D9/Iridis-runs/directedruns/iridis-folder-copies/'+molecule+'/'
    filefrom = '/media/kselvon/7A82B43582B3F42B/iridisruns2019/iridis-folder-copies/'+molecule+'/'
    filefromfold = 'drag'+inout+'z'+str(zval)+'/'
    filefromfold59 = 'drag'+inout+'z'+str(zval)+'_59h/'
    infilename = molecule+'DMPC_drag'+inout+'.pdb'
    
    
    fileto = '/media/kselvon/7A82B43582B3F42B/iridisruns2019/trajectoriesForMD/'
    outfilename = molecule+'_DG_topol_z'+str(zval)+'.pdb'
    
    
    #check directories exist and make them     
    if not os.path.exists(fileto+molecule):
        os.mkdir(fileto+molecule)
    if not os.path.exists(fileto+molecule+'/'+inout):
        os.mkdir(fileto+molecule+'/'+inout)
    if not os.path.exists(fileto+molecule+'/'+inout+'/z'+str(zval)):
        os.mkdir(fileto+molecule+'/'+inout+'/z'+str(zval))
        
    if os.path.exists(filefrom+filefromfold59+infilename):
         lines = open(filefrom+filefromfold59+infilename).readlines()
         shutil.copyfile(filefrom+filefromfold59+molecule+'_drag'+inout+'_xtc.xtc',fileto+molecule+'/'+inout+'/z'+str(zval)+'/'+molecule+'_drag'+inout+'_xtc.xtc') 
         print('reading in 59h')
    else:
        lines = open(filefrom+filefromfold+infilename).readlines()
        shutil.copyfile(filefrom+f:q!
                        ilefromfold+molecule+'_drag'+inout+'_xtc.xtc',fileto+molecule+'/'+inout+'/z'+str(zval)+'/'+molecule+'_drag'+inout+'_xtc.xtc')    
        print('reading in normal')
    
    
    open(fileto+molecule+'/'+inout+'/z'+str(zval)+'/'+outfilename,'w').writelines(lines[0:endline])
    
    
    #copy over the xtc

    
     
    #chloramphenicol 6186 
    #keto6187
    #digoxin endline 6273
    #verapamil 6225
     #zomig 6196
    return     
    
